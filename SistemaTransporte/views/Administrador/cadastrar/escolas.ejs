<% include ../../partials/header %>
    <div class="container">
        <div class="content">
            <div class="container_12">
                <h2>Cadastrar Escolas/Instituições</h2>
                <form method="post">
                <input type="text" name="nome" id="" class="form" placeholder="Nome" required autofocus>
                <input type="text" name="rua" id="" class="form" placeholder="Rua" required autofocus>
                <input type="text" name="bairro" id="" class="form" placeholder="Bairro" required autofocus>
                <input type="text" name="telefone" id="" class="form" placeholder="Telefone" required autofocus>
                <input type="text" name="contato" id="" class="form" placeholder="Contato" required autofocus>
                <input class="form" hidden>
                <div class="form">
                    <input type="checkbox" class="checkbox form" id="manha" value="manha" name="check" />
                    <label for="manha" class="font">Manhã: 07:00 - 12:00</label>
                </div>
                <div class="form">
                    <input type="checkbox" class="checkbox form" id="tarde" value="tarde" name="check" />
                    <label for="tarde" class="font">Tarde: 13:00 - 17:00</label>
                </div>
                <div class="form">
                    <input type="checkbox" class="checkbox form" id="noite" value="noite" name="check" />
                    <label for="noite" class="font">Noite: 19:00 - 22:00</label>
                </div>
                <button class="btnhome btnsingin typetext" type="submit"> <span>Enviar</span></button>
                </form>
            </div>       
        </div>        
    </div>
<% include ../../partials/footer %>